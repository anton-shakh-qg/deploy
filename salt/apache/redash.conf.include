# vi: ft=apache

#  timeout of 10 minutes copied from other sites as a precaution for big downloads of data, but not yet seen evidence it is specifically needed for redash
Timeout 600
ProxyTimeout 600

<Location "/">
    ProxyPass http://127.0.0.1:9090/ timeout=600
</Location>

<Location "/.well-known/acme-challenge">
    ProxyPass !
    Require all granted
</Location>
